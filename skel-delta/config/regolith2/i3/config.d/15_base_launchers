###############################################################################
# Launcher dialogs
###############################################################################

## Launch // Terminal // <> Enter ##
set_from_resource $i3-wm.binding.terminal i3-wm.binding.terminal Return
bindsym $mod+$i3-wm.binding.terminal exec --no-startup-id /usr/bin/x-terminal-emulator

## Launch // IDE // Primary <><Shift> Enter ##
set_from_resource $i3-wm.binding.ide.primary i3-wm.binding.ide.primary F2
bindsym $mod+$i3-wm.binding.ide.primary exec --no-startup-id code

## Launch // IDE // Secondary <><Shift> Enter ##
set_from_resource $i3-wm.binding.ide.secondary i3-wm.binding.ide.secondary Shift+F2
bindsym $mod+$i3-wm.binding.ide.secondary exec --no-startup-id atom

## Launch // Graphix // Primary <><Shift> Enter ##
set_from_resource $i3-wm.binding.graphix.primary i3-wm.binding.graphix.primary F3
bindsym $mod+$i3-wm.binding.graphix.primary exec --no-startup-id inkscape

## Launch // Graphix // Secondary <><Shift> Enter ##
set_from_resource $i3-wm.binding.graphix.secondary i3-wm.binding.graphix.secondary Shift+F3
bindsym $mod+$i3-wm.binding.graphix.secondary exec --no-startup-id gimp

## Launch // Office // Primary <><Shift> Enter ##
set_from_resource $i3-wm.binding.office.primary i3-wm.binding.office.primary F4
bindsym $mod+$i3-wm.binding.office.primary exec --no-startup-id lowriter

## Launch // Office // Secondary <><Shift> Enter ##
set_from_resource $i3-wm.binding.office.secondary i3-wm.binding.office.secondary Shift+F4
bindsym $mod+$i3-wm.binding.office.secondary exec --no-startup-id localc

## Launch // Music // Primary <><Shift> Enter ##
